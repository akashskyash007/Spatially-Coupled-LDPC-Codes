:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55af749c5850 .scope module, "UP_COUNTER_TEST_BENCH" "UP_COUNTER_TEST_BENCH" 2 3;
 .timescale -9 -12;
v0x55af749da8e0_0 .var "clk", 0 0;
v0x55af749da9a0_0 .var "condition_check", 0 0;
v0x55af749daa70_0 .net "count", 1 0, v0x55af749da3d0_0;  1 drivers
v0x55af749dab70_0 .var "min_index", 1 0;
v0x55af749dac40_0 .var "reset", 0 0;
S_0x55af749c59e0 .scope module, "DUT" "UP_COUNTER" 2 8, 3 6 0, S_0x55af749c5850;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "condition_check";
    .port_info 3 /INPUT 2 "min_index";
    .port_info 4 /OUTPUT 2 "count";
L_0x55af749dae80 .functor AND 1, L_0x55af749dad10, v0x55af749da9a0_0, C4<1>, C4<1>;
L_0x7f63dc4c6018 .functor BUFT 1, C4<00>, C4<0>, C4<0>, C4<0>;
v0x55af74989cf0_0 .net/2u *"_ivl_0", 1 0, L_0x7f63dc4c6018;  1 drivers
v0x55af749da180_0 .net *"_ivl_2", 0 0, L_0x55af749dad10;  1 drivers
v0x55af749da240_0 .net "clk", 0 0, v0x55af749da8e0_0;  1 drivers
v0x55af749da310_0 .net "condition_check", 0 0, v0x55af749da9a0_0;  1 drivers
v0x55af749da3d0_0 .var "count", 1 0;
v0x55af749da500_0 .var "counter_input", 1 0;
v0x55af749da5e0_0 .net "min_index", 1 0, v0x55af749dab70_0;  1 drivers
v0x55af749da6c0_0 .net "reset", 0 0, v0x55af749dac40_0;  1 drivers
v0x55af749da780_0 .net "reset_check", 0 0, L_0x55af749dae80;  1 drivers
E_0x55af7498a860 .event posedge, v0x55af749da6c0_0, v0x55af749da240_0;
E_0x55af749c34f0 .event edge, v0x55af749da3d0_0, v0x55af749da500_0, v0x55af749da5e0_0, v0x55af749da310_0;
L_0x55af749dad10 .cmp/eq 2, v0x55af749dab70_0, L_0x7f63dc4c6018;
    .scope S_0x55af749c59e0;
T_0 ;
    %wait E_0x55af749c34f0;
    %load/vec4 v0x55af749da3d0_0;
    %addi 1, 0, 2;
    %store/vec4 v0x55af749da500_0, 0, 2;
    %load/vec4 v0x55af749da500_0;
    %load/vec4 v0x55af749da5e0_0;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55af749da310_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x55af749da500_0;
    %addi 1, 0, 2;
    %store/vec4 v0x55af749da500_0, 0, 2;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55af749c59e0;
T_1 ;
    %wait E_0x55af7498a860;
    %load/vec4 v0x55af749da6c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55af749da780_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x55af749da3d0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55af749da3d0_0, 0;
T_1.3 ;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55af749da500_0;
    %assign/vec4 v0x55af749da3d0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55af749c5850;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55af749da8e0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55af749c5850;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x55af749da8e0_0;
    %inv;
    %store/vec4 v0x55af749da8e0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55af749c5850;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "UP_COUNTER.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55af749c5850 {0 0 0};
    %vpi_call 2 17 "$monitor", $time, ": count = %2b", v0x55af749daa70_0 {0 0 0};
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55af749dac40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55af749da9a0_0, 0, 1;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x55af749dab70_0, 0, 2;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55af749dac40_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
