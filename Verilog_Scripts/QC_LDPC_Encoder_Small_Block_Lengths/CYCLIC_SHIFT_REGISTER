#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55eea8b6d4c0 .scope module, "cyclic_shift_register_test_bench" "cyclic_shift_register_test_bench" 2 5;
 .timescale -9 -12;
v0x55eea8b92b00_0 .var "clk", 0 0;
v0x55eea8b92bc0_0 .var "in", 15 0;
v0x55eea8b92c90_0 .var "load", 0 0;
v0x55eea8b92d90_0 .net "out", 15 0, v0x55eea8b92970_0;  1 drivers
S_0x55eea8b7faf0 .scope module, "DUT" "CYCLIC_SHIFT_REGISTER_16BIT" 2 10, 3 5 0, S_0x55eea8b6d4c0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "load";
    .port_info 2 /INPUT 16 "in";
    .port_info 3 /OUTPUT 16 "out";
v0x55eea8b477b0_0 .net "clk", 0 0, v0x55eea8b92b00_0;  1 drivers
v0x55eea8b47ba0_0 .net "in", 15 0, v0x55eea8b92bc0_0;  1 drivers
v0x55eea8b928a0_0 .net "load", 0 0, v0x55eea8b92c90_0;  1 drivers
v0x55eea8b92970_0 .var "out", 15 0;
E_0x55eea8b46710 .event posedge, v0x55eea8b477b0_0;
    .scope S_0x55eea8b7faf0;
T_0 ;
    %wait E_0x55eea8b46710;
    %load/vec4 v0x55eea8b928a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x55eea8b47ba0_0;
    %assign/vec4 v0x55eea8b92970_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55eea8b92970_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x55eea8b92970_0;
    %parti/s 15, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55eea8b92970_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55eea8b6d4c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eea8b92b00_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55eea8b6d4c0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x55eea8b92b00_0;
    %inv;
    %store/vec4 v0x55eea8b92b00_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55eea8b6d4c0;
T_3 ;
    %vpi_call 2 16 "$dumpfile", "CYCLIC_SHIFT_REGISTER_16BIT.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55eea8b6d4c0 {0 0 0};
    %vpi_call 2 18 "$monitor", $time, ": out = %16b", v0x55eea8b92d90_0 {0 0 0};
    %pushi/vec4 48059, 0, 16;
    %store/vec4 v0x55eea8b92bc0_0, 0, 16;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55eea8b92c90_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eea8b92c90_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_cyclic_shift_register_16bit.v";
    "CYCLIC_SHIFT_REGISTER_16BIT.v";
